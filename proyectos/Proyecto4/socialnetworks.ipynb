    "DATA_DIR = pathlib.Path().resolve().joinpath('DATA')\n",
    "ig = dict()\n",
    "set_months = {'June', 'September', 'November'}\n",
    "for path in DATA_DIR.glob('i*2022.csv'):\n",
    "    target_month = re.search(r'instagram(.*)2022', str(path), re.IGNORECASE).group(1)\n",
    "    real_month = max(set_months, key = lambda month: SequenceMatcher(None, target_month, month).ratio())\n",
    "    ig.update({\n",
    "        'ig_' + real_month:\n",
    "        pd.read_csv(path)\n",
    "    })"
   ]
